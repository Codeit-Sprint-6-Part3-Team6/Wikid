{
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "printWidth": 100,
  "trailingComma": "all",
  "singleQuote": false,
  "jsxSingleQuote": false,
  "semi": true,
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@components/",
    "^[./]",
    "^@hooks/",
    "^@utils/",
    "^@context/",
    "^@lib/",
    "^@styles/",
    "^@images/",
    "^@icons/"
  ],
  "importOrderSeparation": false
}
